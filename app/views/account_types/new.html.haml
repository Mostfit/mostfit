
- form_url   =  resource(:account_types, {:return => params[:return]})

= error_messages_for @account_type

= form_for(@account_type, :action => form_url, :class => "form" ) do
  %fieldset
    %h1 Create a Account Type
    = partial "account_types/fields"
    = submit "Create"
    = link_to 'Cancel', url(:account_types)


