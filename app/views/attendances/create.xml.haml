%attendance
  -if not @error.blank?
    %error #{@error}
  - elsif not @attendance.blank?
    %message "Attendance created successfully"
    %id #{@attendance.id}
    %center_id #{@attendance.center_id}
    %client_id #{@attendance.client_id}
    %status #{@attendance.status}
    %date #{@attendance.date}
  
  
