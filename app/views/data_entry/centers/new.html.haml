%h1 Add a center

%h3 Please choose a branch to add a center to

= form_for(@center, :action => url(:controller => :"data_entry/centers", :action => "new")) do
  = select :name => :branch_id, :collection => get_accessible_branches, :value_method => :id, :text_method => :name, :prompt => "Please select a branch"
  = submit 'Next...'